<h1><%= t :listing %> <%= t :sellers %></h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.name %></td>
        <td><%= link_to t(:show), seller %></td>
        <td><%= link_to t(:edit), edit_seller_path(seller) %></td>
        <td><%= link_to t(:delete), seller, method: :delete, data: { confirm: t(:confirm) } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t(:new) + ' ' + t(:seller), new_seller_path %>
